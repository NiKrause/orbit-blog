{
  "name": "le-space-blog",
  "version": "0.2.109",
  "type": "module",
  "files": [
    "dist",
    "src/lib"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/style.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --mode lib",
    "preview": "vite preview",
    "build:relay": "tsc --project tsconfig.json",
    "relay": "npm run build:relay && node dist/relay/index.js",
    "relay:test": "npm run build:relay && DEBUG=libp2p:gossipsub,libp2p:relay:*,le-space:*,-le-space:relay:error node dist/relay/index.js --test",
    "relay:log": "npm run build:relay && DEBUG=libp2p:relay:* node dist/relay/index.js --test",
    "relay:sync-only": "npm run build:relay && ENABLE_GENERAL_LOGS=false ENABLE_SYNC_LOGS=true ENABLE_SYNC_STATS=true node dist/relay/index.js",
    "relay:quiet": "npm run build:relay && ENABLE_GENERAL_LOGS=false ENABLE_SYNC_LOGS=false ENABLE_SYNC_STATS=false node dist/relay/index.js",
    "relay:verbose": "npm run build:relay && ENABLE_GENERAL_LOGS=true ENABLE_SYNC_LOGS=true ENABLE_SYNC_STATS=true LOG_LEVEL_CONNECTION=true LOG_LEVEL_PEER=true LOG_LEVEL_DATABASE=true node dist/relay/index.js",
    "test": "mocha --config .mocharc.cjs test/orbitdb*.test.js",
    "test:e2e": "playwright test tests/BlogSetup.spec.ts tests/BlogSharing.spec.ts tests/BlogSharing-Close-A.spec.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:watch": "mocha --config .mocharc.cjs --watch",
    "ipfs-publish": "npm version patch && npm run build && ./ipfs-publish.sh",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
